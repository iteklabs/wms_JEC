                    success: function(response) {

                        if(response.length > 0){
                            $.each(response, function(index, data) {
                            
                                // alert(productName + '==' +  data.name + '&&' + Pallet + '==' + data.pallet)
                                if(productName == data.name && Level == data.bay){

