    <script>
        function invoice_pending(){
            $.ajax({
                        url: '/pending_data_table',
                        method: 'GET',
                        // data: { isbool: "false" },
                        success: function (response) {
                            var table = document.getElementById("pendingInvoice");

                            let dataOptions = "";

                            for (let index = 0; index < response.length; index++) {
                                const element = response[index];
                                dataOptions += "<tr>";
                                dataOptions += "<td class='text-center'><h3>"+element.invoice+"</h3></td>";
                                dataOptions += "<td class='text-center'><h3><input type='number' class='form-control text-center' id="+element._id+" value='"+element.collection_price+"'></h3></td>";
                                dataOptions += "<td class='text-center'><button type='button' onclick=\"updatePaid('" + element._id + "')\" class='btn btn-primary'>Paid</button></td>";
                                dataOptions += "</tr>";
                            }
                            table.innerHTML = dataOptions;
                            
                        }
                    })
                
        }
        function view(id){
            window.location.href = './sales_sa/view_sales/'+id;
        }
        function invoice_paid(){
            $.ajax({
                url: '/paid_data',
                method: 'GET',
                // data: { isbool: "false" },
                success: function (response) {
                    var table = document.getElementById("LatesInvoice");
                    console.log(response)
                    let dataOptions = "";

                    for (let index = 0; index < response.length; index++) {
                        const element = response[index];
                        dataOptions += "<tr>";
                        dataOptions += "<td class='text-center'><h3>"+element.invoice+"</h3></td>";
                        dataOptions += "<td class='text-center'><h3>"+element.collection_price+"</h3></td>";
                        dataOptions += "<td class='text-center'><button type='button' onclick=\"view('" + element._id + "')\" class='btn btn-success'>View</button></td>";
                        dataOptions += "</tr>";
                    }
                    table.innerHTML = dataOptions;
                    
                }
            })
                
        }

        function updatePaid(id){
            var thevalprice = document.getElementById(id).value;

            $.ajax({
                        url: '/update_data',
                        method: 'POST',
                        data: { id: id, price: thevalprice },
                        success: function (response) {
                            if(response.paid == "True"){
                                Swal.fire(
                                    '',
                                    response.invoice + ' is changed to paid.',
                                    'success'
                                )
                                invoice_pending();
                                invoice_paid();
                                count_paid();
                                count_unpaid();
                            }else{
                                Swal.fire(
                                    '',
                                    'Error',
                                    'error'
                                )
                            }
                        }
                    })
        }


        function count_paid(){
            $.ajax({
                url: '/paid_data_table',
                method: 'GET',
                success: function (response) {
                    document.getElementById("paid_data_invoice").innerHTML = response.length;
                }
            })
        }


        function count_unpaid(){
            $.ajax({
                url: '/pending_data',
                method: 'GET',
                success: function (response) {
                    // console.log(response)
                    document.getElementById("unpaid_data_invoice").innerHTML = response.length;
                }
            })
        }
        function count_inv(){
            $.ajax({
                url: '/my_inv',
                method: 'GET',
                success: function (response) {
                    // console.log(response)
                    document.getElementById("inv_mine").innerHTML = response[0].totalQTY;
                }
            })
        }
        
        async function fetchDataAndUpdateChart(fetchFunction) {
            const newData = await fetchFunction(); // Fetch data using the provided fetchFunction
            await fetchFunction(); 
        }


        function startDataRefresh() {
            const pollingInterval = 5000;
            // Call fetchDataAndUpdateChart for each data source and its corresponding chart
            setInterval(() => fetchDataAndUpdateChart(invoice_pending), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(invoice_paid), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(count_paid), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(count_unpaid), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(avg_data), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(count_inv), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(fetchDataFG), pollingInterval);
            setInterval(() => fetchDataAndUpdateChart(fetchDataStocks), pollingInterval);
        }
        
        function avg_data(){
            $.ajax({
                url: '/avg_data',
