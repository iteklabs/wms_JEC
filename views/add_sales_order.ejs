                        success: function (response) {

                            // console.log(response.volume_discount)
                            var theradio = document.getElementsByName("volume_deals_type");
                            var type_discount = document.getElementById("type_discount");
                            if(type_discount.value == "vol_deals"){
                                for (let a = 0; a <= theradio.length -1; a++) {
                                const choose_data = theradio[a];
                                console.log(choose_data.value + " <> " + choose_data.checked)
                                if(choose_data.value == "appl" && choose_data.checked == true){
                                    var price = document.getElementsByName("price");
                                    for (let i = 0; i <= price.length - 1; i++) {
                                        const element_detl = price[i];
                                        var theQTY = document.getElementById("quantity"+i).value;
                                        for (let index = 0; index <= response.volume_discount.length - 1; index++) {
                                            const element = response.volume_discount[index];
                                            

                                            if(element.min_car <= theQTY && element.max_car >= theQTY){
                                                // console.log(element.min_car + " <> " + element.max_car + " <> " + element.discount_price)
                                                document.getElementById("dicount_price"+i).value = element.discount_price;
                                                document.getElementById("dicount_price"+i).readOnly = true;
                                                computePerPrice(i)
                                            }
                                        }

                                    }
                                }else if(choose_data.value == "uappl" && choose_data.checked == true){
                                    var price = document.getElementsByName("price");
                                    for (let i = 0; i <= price.length - 1; i++) {
                                        const element_detl = price[i];
                                        var theQTY = document.getElementById("quantity"+i).value;
                                        document.getElementById("dicount_price"+i).value = 0;
                                    }
                                }
                                // console.log()
                            }
                            
                            }
                            
                            
                        }
                    })

                }
                function data_grossprice(){
                    var price = document.getElementsByName("price");
                    for (let i = 0; i <= price.length - 1; i++) {
                        const element_detl = price[i];
                        var data_check = document.getElementsByName("age");
                        var gross_price = element_detl.value;
                        var thePriceDiscount = 0;
                        var deeductPrice =0;
                        var total_discount = 0;
                        for (let index = 0; index <= data_check.length - 1; index++) {
                            const element = data_check[index];
                            if (element.checked == true) {
                                var discount_percentage = parseFloat(element.value) / 100;
                                var discount_amount = gross_price * discount_percentage;
                                gross_price -= discount_amount;
                                total_discount += discount_amount;
                        }
                        document.getElementById("dicount_price"+i).value = total_discount.toFixed(2);
                        // document.getElementById("totalPrice"+i).value = gross_price.toFixed(2);
                        computePerPrice(i);
                        document.getElementById("dicount_price"+i).readOnly = true;
                        
                    }

                }
                }


