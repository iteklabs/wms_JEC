        <script>
        $(function(){
            var dtToday = new Date();

            var month = dtToday.getMonth() + 1;
            var day = dtToday.getDate();
            var year = dtToday.getFullYear();
            if(month < 10)
            month = '0' + month.toString();
            if(day < 10)
            day = '0' + day.toString();

            var maxDate = year + '-' + month + '-' + day;
            $('#date').attr('max', maxDate);
        });
        function showTextbox() {
            var dropdown = document.getElementById("choice-dropdown");
            var selectedChoice = dropdown.value;
    
            var primaryTextbox = document.getElementById("primary-textbox");
            var secondaryTextbox = document.getElementById("secondary-textbox");
    
            if (selectedChoice === "Primary") {
            primaryTextbox.style.display = "block";
            secondaryTextbox.style.display = "none";
            document.getElementById("product_code").focus()
            } else if (selectedChoice === "Secondary") {
            primaryTextbox.style.display = "none";
            secondaryTextbox.style.display = "block";
            } else {
            primaryTextbox.style.display = "none";
            secondaryTextbox.style.display = "none";
            }
        }
        
        function SelectRoom() {
            var variable = document.getElementById("adjustment_warehouse_name").value;
            var selectRoom = $('#showRoom');

            $.ajax({
                url: '/warehousemap_Income/Rooms_data',
                method: 'POST',
                data: { warehouse_name: variable },
                success: function (response) {
                    selectRoom.empty();

                    $.each(response, function (index, data) {
                        var roomName = data.room_name;
                        var roomCode = data.room_name;

                        // Create checkbox element instead of option
                        var checkboxElement = $('<input>')
                            .attr('type', 'checkbox')
                            .addClass('form-check-input')
                            .val(roomName)
                            .attr("name", "room_name")
                            .attr("id", "room_data"+index)
                            .attr('roomcode', roomCode);

                        // Create label for the checkbox
                        var labelElement = $('<h5>').text(roomName).prepend(checkboxElement);

                        // Append the label to the container
                        selectRoom.append(labelElement);

                        labelElement.click(function () {
                            checkboxElement.prop('checked', !checkboxElement.prop('checked'));
                        });

                        checkboxElement.click(function (event) {
                            event.stopPropagation();
                        });

                        // BayBin();
                        checkboxElement.click(function () {
                            // BayBin(index);
                        });
                    });
                }
            });
        }

        function dataCheckingRack(room, level){
            var dataLoc;
            if(room == "Receiving Area"){
                dataLoc = "DRF" + level.slice(5);
            }else if(room == "Shelves"){
                dataLoc = "DB1SF" + level.slice(5);
            }else{
                dataLoc = level;
            }
            
            // console.log(room + " <> " + level )
            return dataLoc
        }

        function myFunctionCompute(row) {
        var adjustQtyInput = document.getElementById("adjust_qty" + row);
        var newAdjustQtyInput = document.getElementById("new_adjust_qty" + row);
        var selectedOption = document.getElementById("types" + row).value;
        
        var adjustQty = parseFloat(adjustQtyInput.value);
        var stock = parseFloat(document.getElementById("stock" + row).value);

            if (!isNaN(adjustQty) && !isNaN(stock)) {
                var result;

                if (selectedOption === "add") {
                result = stock + adjustQty;
                } else if (selectedOption === "minus") {
                result = stock - adjustQty;
                }

                if (!isNaN(result)) {
                newAdjustQtyInput.value = result;
                } else {
                newAdjustQtyInput.value = "";
                }
            } else {
                newAdjustQtyInput.value = "";
            }
        }

        function ValidationForMaxQTY(product_code, i){
            var prod_qty = document.getElementById("adjust_qty"+i).value;
            var stocks_qty = document.getElementById("MaxStocks_data"+product_code+i).value;
            var curStocks = document.getElementById("stock"+i).value;
            var new_adjust_qty = document.getElementById("new_adjust_qty"+i).value; 
            var selectedOption = document.getElementById("types" + i).value;
            var TotalStocksRemain = parseInt(stocks_qty) - parseInt(curStocks)


            if (selectedOption === "add") {
            if(parseInt(prod_qty) >  parseInt(TotalStocksRemain)){
                Swal.fire(
                'Sorry',
                'Sorry the maximum quantity has been reached.',
                'warning'
                )
                document.getElementById("adjust_qty"+i).value = "";
                document.getElementById("new_adjust_qty"+i).value = "";
            }
            } else if (selectedOption === "minus") {
                // alert(new_adjust_qty)
                if( parseInt(curStocks) < parseInt(prod_qty)){
                    Swal.fire(
                    'Sorry',
                    'Sorry the maximum quantity has been reached.',
                    'warning'
                    )
                    document.getElementById("adjust_qty"+i).value = "";
                    document.getElementById("new_adjust_qty"+i).value = "";
                }
            }
            
        }

        function removeSelfRow(event) {
       
            var row = $(event.target).closest('tr');
            
            row.remove();
        }

        $(document).on('click', 'button#remove', function(event) {
            removeSelfRow(event);
        });
        

        function ComputeConversion(x, c){
            var stock = document.getElementById("stock"+x);
            var types = document.getElementById("types"+x);
            var unit = document.getElementById("unit"+x);
            var new_adjust_qty = document.getElementById("new_adjust_qty"+x);
            var prod_cat = document.getElementById("prod_cat"+x);
            var adjust_qty = document.getElementById("adjust_qty"+x);
            var Primary_Units = document.getElementById("Primary_Units"+x).value;
            var Secondary_units = document.getElementById("Secondary_units"+x).value;
            // alert(stock.value + " <> " + types.value + " <> " + unit.value + " <> " + new_adjust_qty.value + " <> " + prod_cat.value + " <> " + adjust_qty.value + " <> " + Primary_Units.value + " <> " +Secondary_units.value + " <> " + x + " <> " +c );
            var TotalQtyStock =0;
            var UOM;
            TotalQtyStock = stock.value;
            UOM = Primary_Units;
            if(prod_cat.value == "S"){
                TotalQtyStock = stock.value * c;
                UOM = Secondary_units;
            }
            stock.value = TotalQtyStock;
            unit.value = UOM;
            

        }
        
        
        function ComputeConversionI(x, c){
            var stock = document.getElementById("stock"+x);
            var types = document.getElementById("types"+x);
            var unit = document.getElementById("unit"+x);
            var new_adjust_qty = document.getElementById("new_adjust_qty"+x);
            var prod_cat = document.getElementById("prod_cat"+x);
            var adjust_qty = document.getElementById("adjust_qty"+x);
            var Primary_Units = document.getElementById("Primary_Units"+x).value;
            var Secondary_units = document.getElementById("Secondary_units"+x).value;
            var New_Qty_Converted = document.getElementById("New_Qty_Converted"+x);
            var New_Qty_Converted_adj = document.getElementById("New_Qty_Converted_adj"+x);
   
            var TotalQtyStock =0;
            var UOM;
            var TotalQtyAdjustment = 0;
            TotalQtyStock = stock.value;
            TotalQtyAdjustment = adjust_qty.value;
            UOM = Primary_Units;
            if(prod_cat.value == "S"){
                TotalQtyStock = new_adjust_qty.value / c;
                TotalQtyAdjustment = adjust_qty.value / c
            }
            New_Qty_Converted.value = TotalQtyStock;
            New_Qty_Converted_adj.value = TotalQtyAdjustment;

        }
        $(document).ready(function() {
            
            var x= $("#last_increment").val()
            $('#product_code').on('keydown', function(e) {
            if (e.which === 13) {
              e.preventDefault();
              return false;
            }
          });
          // AJAX request when the value in the text box changes
          $('#product_code').on('input', function() {
            var productCode = $(this).val(); // Get the entered product code
            var Warehouses = $("#adjustment_warehouse_name").val(); // Get the entered product code
            var Rooms = $("#Room_name").val(); // Get the entered product code
            
            // Check if the product code already exists in the table
            var isProductExists = false;
            $('#tblPage_new tr').each(function() {
              var existingProductCode = $(this).find('td:nth-child(2)').text();
              if (existingProductCode === productCode) {
                isProductExists = true;
                return false; // Exit the loop if the product is found
              }
            });
          
            if (!isProductExists) {
                var rooms_data = document.getElementsByName("room_name");
                var RoomAssign = [];

                for (let index = 0; index < rooms_data.length; index++) {
                    const element = rooms_data[index];

                    if (element.checked) {
                        RoomAssign.push(element.value);
                    }
                }

                var output = RoomAssign.join(',');
              $.ajax({
                url: '/adjustment/barcode_scanner', 
                method: 'POST',
                data: { primary_code: productCode, warehouse_data: Warehouses , room_data: Rooms, Roomslist:output }, 
                success: function(response) {
                  // Populate the table with the received data
                 
                  $.each(response, function(index, data1) {

                    data1.forEach((data) => {
                        // console.log(data)
                        if(data.product_stock > 0){
                            var row = $('<tr>');
                            const levelData = data.level === null ? 0 : data.level;
                            const isleData =  data.isle === null ? 0 : data.isle;
                            const palletData =  data.pallet === null ? 0 : data.pallet; 
                            var dataLocation = dataCheckingRack(data.roomNamed, data.storage+data.rack+data.level+data.isle+data.type[0]+data.pallet);

                            
                            row.append($('<td>').html('<input type="text" class="form-control" style="width: 130px;" name="prod_code" value="' + data.product_code + '" readonly>'));
                            row.append($('<td>').html('<input type="text" class="form-control" style="width: 400px;" name="prod_name" value="' + data.name + '" readonly>'));
                            row.append($('<td>').html('<input type="text" class="form-control prime1" style="width: 150px;" name="prime1" id="prime1' + x + '" value="'+data.primary_code+'" readonly>'));
                            row.append($('<td>').html('<input type="text" class="form-control second1" style="width: 150px;" name="second1" id="second1' + x + '" value="'+data.secondary_code+'" readonly>'));
                            row.append($('<td>').html('<input type="number" class="form-control adjust_qty" style="width: 80px;" name="adjust_qty" id="adjust_qty' + x + '" oninput="myFunctionCompute('+ x +')" onkeyup="ValidationForMaxQTY('+ data.product_code +', ' + x + '); ComputeConversionI('+x+', '+data.maxPerUnit+');" value="">'));
                            row.append($('<td>').html('<input type="text" class="form-control batch_code" style="width: 150px;" name="batch_code" id="batch_code' + x + '" value="'+data.batch_code+'">'));    
                            row.append($('<td>').html('<input type="date" class="form-control" style="width: 150px;" name="production_date" value="' + data.production_date + '" readonly>'));
                            row.append($('<td>').html('<input type="date" class="form-control" style="width: 150px;" name="expiry_date" value="' + data.expiry_date + '" readonly>'));
